"""
Node is defined as
self.left (the left child of the node)
self.right (the right child of the node)
self.info (the value of the node)
"""
def preOrder(root):
    def _preOrder(root,t):
        if root:
            t += str(root.info) + ' '
            t = _preOrder(root.left,t)
            t = _preOrder(root.right,t)
        return t

    if root:
        return print(_preOrder(root,''))
